{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "api/webhook.js",
      "methods": ["POST", "GET"]
    }
  ],
  "functions": {
    "api/webhook.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 